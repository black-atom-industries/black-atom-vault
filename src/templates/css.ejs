:root {
  /* Primaries */
  --black-atom-<%= theme.meta.key %>-primary-0: <%= theme.primaries[0] %>;
  --black-atom-<%= theme.meta.key %>-primary-1: <%= theme.primaries[1] %>;
  --black-atom-<%= theme.meta.key %>-primary-2: <%= theme.primaries[2] %>;
  --black-atom-<%= theme.meta.key %>-primary-3: <%= theme.primaries[3] %>;
  --black-atom-<%= theme.meta.key %>-primary-4: <%= theme.primaries[4] %>;
  --black-atom-<%= theme.meta.key %>-primary-5: <%= theme.primaries[5] %>;
  --black-atom-<%= theme.meta.key %>-primary-6: <%= theme.primaries[6] %>;
  --black-atom-<%= theme.meta.key %>-primary-7: <%= theme.primaries[7] %>;
  --black-atom-<%= theme.meta.key %>-primary-8: <%= theme.primaries[8] %>;
  --black-atom-<%= theme.meta.key %>-primary-9: <%= theme.primaries[9] %>;
  --black-atom-<%= theme.meta.key %>-primary-10: <%= theme.primaries[10] %>;
  --black-atom-<%= theme.meta.key %>-primary-11: <%= theme.primaries[11] %>;

  /* Palette */
  --black-atom-<%= theme.meta.key %>-black: <%= theme.palette.black %>;
  --black-atom-<%= theme.meta.key %>-gray: <%= theme.palette.gray %>;
  --black-atom-<%= theme.meta.key %>-dark-red: <%= theme.palette.darkRed %>;
  --black-atom-<%= theme.meta.key %>-red: <%= theme.palette.red %>;
  --black-atom-<%= theme.meta.key %>-dark-green: <%= theme.palette.darkGreen %>;
  --black-atom-<%= theme.meta.key %>-green: <%= theme.palette.green %>;
  --black-atom-<%= theme.meta.key %>-dark-yellow: <%= theme.palette.darkYellow %>;
  --black-atom-<%= theme.meta.key %>-yellow: <%= theme.palette.yellow %>;
  --black-atom-<%= theme.meta.key %>-dark-blue: <%= theme.palette.darkBlue %>;
  --black-atom-<%= theme.meta.key %>-blue: <%= theme.palette.blue %>;
  --black-atom-<%= theme.meta.key %>-dark-magenta: <%= theme.palette.darkMagenta %>;
  --black-atom-<%= theme.meta.key %>-magenta: <%= theme.palette.magenta %>;
  --black-atom-<%= theme.meta.key %>-dark-cyan: <%= theme.palette.darkCyan %>;
  --black-atom-<%= theme.meta.key %>-cyan: <%= theme.palette.cyan %>;
  --black-atom-<%= theme.meta.key %>-light-gray: <%= theme.palette.lightGray %>;
  --black-atom-<%= theme.meta.key %>-white: <%= theme.palette.white %>;

  /* Semantics */
  --black-atom-<%= theme.meta.key %>-bg-primary-dark: <%= theme.semantics.bg.primary.dark %>;
  --black-atom-<%= theme.meta.key %>-bg-primary-main: <%= theme.semantics.bg.primary.main %>;
  --black-atom-<%= theme.meta.key %>-bg-primary-light: <%= theme.semantics.bg.primary.light %>;
  --black-atom-<%= theme.meta.key %>-bg-secondary-dark: <%= theme.semantics.bg.secondary.dark %>;
  --black-atom-<%= theme.meta.key %>-bg-secondary-main: <%= theme.semantics.bg.secondary.main %>;
  --black-atom-<%= theme.meta.key %>-bg-secondary-light: <%= theme.semantics.bg.secondary.light %>;
  --black-atom-<%= theme.meta.key %>-bg-active: <%= theme.semantics.bg.active %>;
  --black-atom-<%= theme.meta.key %>-bg-match-active: <%= theme.semantics.bg.match.active %>;
  --black-atom-<%= theme.meta.key %>-bg-match-passive: <%= theme.semantics.bg.match.passive %>;
  --black-atom-<%= theme.meta.key %>-fg-primary-dark: <%= theme.semantics.fg.primary.dark %>;
  --black-atom-<%= theme.meta.key %>-fg-primary-main: <%= theme.semantics.fg.primary.main %>;
  --black-atom-<%= theme.meta.key %>-fg-primary-light: <%= theme.semantics.fg.primary.light %>;
  --black-atom-<%= theme.meta.key %>-fg-secondary-dark: <%= theme.semantics.fg.secondary.dark %>;
  --black-atom-<%= theme.meta.key %>-fg-secondary-main: <%= theme.semantics.fg.secondary.main %>;
  --black-atom-<%= theme.meta.key %>-fg-secondary-light: <%= theme.semantics.fg.secondary.light %>;
  --black-atom-<%= theme.meta.key %>-fg-active: <%= theme.semantics.fg.active %>;
  --black-atom-<%= theme.meta.key %>-fg-invert: <%= theme.semantics.fg.invert %>;
  --black-atom-<%= theme.meta.key %>-fg-neutral: <%= theme.semantics.fg.neutral %>;

  /* Syntax */
  --black-atom-<%= theme.meta.key %>-syntax-default: <%= theme.syntax.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-variable: <%= theme.syntax.variable.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-variable-builtin: <%= theme.syntax.variable.builtin %>;
  --black-atom-<%= theme.meta.key %>-syntax-constant: <%= theme.syntax.constant.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-constant-builtin: <%= theme.syntax.constant.builtin %>;
  --black-atom-<%= theme.meta.key %>-syntax-string: <%= theme.syntax.string.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-comment: <%= theme.syntax.comment.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-keyword: <%= theme.syntax.keyword.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-function: <%= theme.syntax.func.default %>;
  --black-atom-<%= theme.meta.key %>-syntax-type: <%= theme.syntax.type.default %>;
}
